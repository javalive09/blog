transition 闪烁

                if(!TextUtils.isEmpty(bundle.getString("full"))) {
                    mWebView.loadUrl("javascript:var stream = document.getElementsByTagName('video'); for (var i = 0;"
                            + " i <stream.length; i++) {\n"
                            + " \tif(!stream[i].paused) {\n"
                            + " \t\tstream[i].webkitEnterFullScreen();\n"
                            + " \t}\n"
                            + " }");
                    return;
                }

                if(!TextUtils.isEmpty(bundle.getString("close"))) {
                    mWebView.loadUrl("javascript:document.webkitExitFullscreen()");
                    return;
                }